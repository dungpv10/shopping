{
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]
      }],
      "react/forbid-prop-types": 0,
      "quotes" : ["error", "double"],
      "react/prefer-stateless-function": [0, { "ignorePureComponents": false }],
      "indent": ["error", 4],
      "react/jsx-indent" : [2, 4],
      "max-len" : ["error", 120, 4]
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    }
}